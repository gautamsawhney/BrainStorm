<h2> ADD LEVEL </h2>

	<% #(@level.new_record?) ? "Add Level #{current_level + 1}" : "Editing Level #{@level.id}" %>

<%= form_for @level do |f| %>

  <%= f.label :question %>
  <%= f.text_field :question %>

  <%= f.label :answer %>
  <%= f.text_field :answer%>

  <%=f.submit "Create" %>

<% end %>